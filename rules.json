{
    "ignore_files": [
        ".git/",
        ".DS_Store",
        ".idea/",
        ".vscode/",
        "venv/",
        "env/",
        "build/",
        "dist/",
        "*.egg-info/",
        "template.json"
    ],
    "rules": [
